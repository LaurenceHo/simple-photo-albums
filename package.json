{
  "name": "simple-photo-albums",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port=9000 --open",
    "format": "prettier --write src/ --ignore-unknown --ignore-path .gitignore",
    "lint": "eslint --fix",
    "msw": "vite --mode MSW --open",
    "prepare": "husky",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@primevue/themes": "^4.5.4",
    "@tabler/icons-vue": "^3.37.1",
    "@tanstack/vue-query": "^5.92.9",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "dompurify": "^3.3.1",
    "embla-carousel-vue": "^8.6.0",
    "exifreader": "^4.36.2",
    "mapbox-gl": "^3.18.1",
    "pinia": "^3.0.4",
    "primevue": "^4.5.4",
    "radash": "^12.1.1",
    "tailwindcss": "^4.2.0",
    "tailwindcss-primeui": "0.6.1",
    "vue": "^3.5.28",
    "vue-router": "5.0.3",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@tailwindcss/vite": "^4.2.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "27.0.0",
    "@types/node": "25.3.0",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/eslint-plugin": "^1.6.9",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.7.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "0.8.1",
    "eslint": "10.0.1",
    "eslint-plugin-vue": "^10.8.0",
    "husky": "^9.1.7",
    "jsdom": "28.1.0",
    "lint-staged": "16.2.7",
    "msw": "^2.12.10",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "sass-embedded": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "7.3.1",
    "vite-plugin-vue-devtools": "8.0.6",
    "vitest": "4.0.18",
    "vue-tsc": "3.2.5"
  },
  "engines": {
    "node": "^22",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
