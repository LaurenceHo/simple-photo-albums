{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "dist/worker.js",
  "files": [
    "dist/**/*.*",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "format": "prettier --write src/ test/ --ignore-unknown --ignore-path .gitignore",
    "lint": "eslint --fix",
    "start:wrangler": "wrangler dev --local",
    "test:server": "NODE_OPTIONS='--require ./test/slowbuffer-polyfill.cjs' vitest run",
    "types:server": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.995.0",
    "@aws-sdk/s3-request-presigner": "^3.995.0",
    "@hono/zod-validator": "^0.7.6",
    "@xmldom/xmldom": "^0.8.11",
    "google-auth-library": "10.5.0",
    "hono": "^4.12.1",
    "jose": "^6.1.3",
    "radash": "^12.1.1",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260228.0",
    "@eslint/js": "10.0.1",
    "@tsconfig/node22": "^22.0.5",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "25.3.0",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "form-auto-content": "^3.2.1",
    "globals": "17.3.0",
    "lint-staged": "16.2.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": "^22",
    "npm": ">= 10",
    "yarn": ">= 4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
